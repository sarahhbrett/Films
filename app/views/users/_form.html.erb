<%= form_for(@user) do |f| %>
  <% if @user.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@user.errors.count, "error") %> prohibited this user from being saved:</h2>

      <ul>
      <% @user.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :email %><br>
    <%= f.collection_select :email, County.all. :name, :name %>
  </div>
  
  <div class="field">
 <%= f.label :password_confirmation %><br />
 <%= f.password_field :password_confirmation %>
 </div>
  
  <div class="field">
    <%= f.label :dob %><br>
    <%= f.date_select :dob, {:start_year => 1920, :include_blank => true} %>
  </div>
  
  <div class="field">
    <%= f.label :photo %><br>
    <%= f.text_field :photo %>
  </div>
  
  <div class="field">
    <%= f.label :password %><br /> Password must be 8 characters long
    <%= f.password_field :password %>
  </div>
  
  <div class="field">
    <%= f.label :password_confimation %><br>
    <%= f.password_field :password_confimation %>
  </div>
  <div class="actions">
    <%= f.submit  "Sign up"%>
  </div>
<% end %>
